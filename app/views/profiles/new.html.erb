<%= form_for Profile.new, url: user_profile_path, html: { multipart: true } do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :bio %>
  <%= f.text_field :bio %>
  <%= f.file_field :image %>
  <%= f.submit %>
<% end %>
